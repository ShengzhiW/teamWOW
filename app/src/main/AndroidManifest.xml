<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamwow.onewalk">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service android:name=".StepCounterService" />

        <activity
            android:name=".Login"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChallengePage"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".ShopPage" />
        <activity android:name=".StepCounter" />
        <activity android:name=".LeaderboardPage" />
        <activity android:name=".SettingsPage" />
        <activity android:name=".PrivacyPage" />
    </application>

</manifest>